{
  "private": true,
  "packageManager": "pnpm@9.9.0",
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "build:turbo": "turbo run build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "commit": "git-cz",
    "format": "prettier --write .",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix",
    "lint:style": "stylelint \"**/*.{vue,css,scss}\" --fix",
    "prepare": "husky install",
    "test": "pnpm -r test",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:unit": "vitest",
    "tree": "tree -I 'node_modules|.git|.DS_Store|dist'",
    "version": "changeset version",
    "version:pre": "changeset pre enter alpha",
    "version:pre:exit": "changeset pre exit",
    "publish": "pnpm build && changeset publish",
    "release": "pnpm build && pnpm version && pnpm publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@eslint/eslintrc": "^3.2.0",
    "@spark/internal-lint": "workspace:^",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.5",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.2.0",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^4.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.17.0",
    "fast-glob": "^3.3.3",
    "husky": "^8.0.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.0.0",
    "stylelint": "^16.12.0",
    "turbo": "^2.3.3",
    "vitest": "^1.2.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,vue}": [
      "stylelint --fix"
    ]
  }
}
